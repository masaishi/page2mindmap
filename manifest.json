{
	"manifest_version": 3,
	"name": "page2mindmap",
	"version": "1.3",
	"description": "Transform web pages into mindmaps with a click, using GPT-4 for content summarization.",
	"permissions": [
		"activeTab",
		"storage",
		"contextMenus",
		"scripting",
		"tabs",
		"offscreen",
		"notifications",
		"downloads"
	],
	"action": {
		"default_icon": {
			"16": "images/icon16.png",
			"32": "images/icon32.png",
			"48": "images/icon48.png",
			"128": "images/icon128.png"
		},
		"default_title": "page2mindmap"
	},
	"author": "mwishiha@ucsc.edu",
	"background": {
		"service_worker": "background.js"
	},
	"icons": {
		"16": "images/icon16.png",
		"32": "images/icon32.png",
		"48": "images/icon48.png",
		"128": "images/icon128.png"
	},
	"web_accessible_resources": [
		{
			"resources": ["apikey_input.html", "apikey_input.js"],
			"matches": ["<all_urls>"]
		}
	]
}
