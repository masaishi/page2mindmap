{
	"manifest_version": 3,
	"name": "page2mindmap",
	"version": "1.0.0",
	"description": "Transform web pages into mindmaps with a click, using GPT-4 for content summarization.",
	"permissions": [
		"activeTab",
		"storage",
		"contextMenus",
		"scripting",
		"offscreen",
		"downloads",
		"notifications"
	],
	"action": {
		"default_icon": {
			"16": "icons/icon16.png",
			"32": "icons/icon32.png",
			"48": "icons/icon48.png",
			"128": "icons/icon128.png"
		},
		"default_title": "page2mindmap"
	},
	"author": "mwishiha@ucsc.edu",
	"background": {
		"service_worker": "background.js"
	},
	"icons": {
		"16": "icons/icon16.png",
		"32": "icons/icon32.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	}
}
